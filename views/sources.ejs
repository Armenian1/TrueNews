<% include ./partials/header %>

<h1>This is the sources page!</h1>

<form action="/news/sources" method="POST">
    <div>
        <input id="cnn" type="checkbox" name="sources" value="cnn">
        <label for="cnn">CNN</label>
    </div>
    <div>
        <input id="foxnews" type="checkbox" name="sources" value="fox-news">
        <label for="foxnews">Fox News</label>
    </div>
    <div>
        <input id="nytimes" type="checkbox" name="sources" value="the-new-york-times">
        <label for="nytimes">New York Times</label>
    </div>
    <div>
        <button type="submit">Submit</button>
    </div>
</form>

<% include ./partials/footer %>